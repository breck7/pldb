 example
  program define fizzbuzz
  	args x
  	forvalues i = 1(1)`x' {
  		if mod(`i',15) == 0 {
  			display "fizzbuzz"
  		}
  		else if mod(`i',5) == 0 {
  			display "buzz"
  		}
  		else if mod(`i',3) == 0 {
  			display "fizz"
  		}
  		else {
  		display `i'
  		}
  	}
  end
