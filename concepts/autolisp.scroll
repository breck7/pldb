   2     (if (setq pnt (getpoint "\nSpecify point: "))
   3         (progn
   4             (entmake
   5                 (list
   6                    '(0 . "POINT")
   7                     (cons 10 (trans pnt 1 0))
   8                 )
   9             )
  10             (entmake
  11                 (list
  12                    '(0 . "TEXT")
  13                     (cons 10 (trans (cons (+ (car pnt) 0.6) (cdr pnt)) 1 0))
  14                     (cons 40 (getvar 'textsize))
  15                     (cons  1 (strcat "X:" (rtos (car pnt)) " Y:" (rtos (cadr pnt))))
