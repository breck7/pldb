  Example
   program Adhoc;
   
   function Add(x, y : Integer) : Integer;
   begin
       Add := x + y
   end;
   
   function Add(s, t : String) : String;
   begin
       Add := Concat(s, t)
   end;
   
   begin
       Writeln(Add(1, 2));                   (* Prints "3"             *)
       Writeln(Add('Hello, ', 'World!'));    (* Prints "Hello, World!" *)
   end.
