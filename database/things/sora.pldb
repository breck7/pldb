example
 func get(node: &mut Foo, k: usize) -> maybe &mut Foo {
     if k == 0 {
         return node // no semicolons, only newlines
     } else if let next = node->next { // node->next is a maybe &mut Foo, this accesses the value of the maybe type.
         return get(next, k-1)
     } else {
         return null
     }
 }

