        }
      };
  
      // initialize multi selector for product
      Event.observe(document, 'dom:loaded', function() {
        new Shopify.OptionSelectors("variant-select", { product: {{ product | json }}, onVariantSelected: selectCallback });
      });
    -->
    </script>
  </ul>
  

