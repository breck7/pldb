 javascript
  compile() {
   const file = this.root.file
   const files = file.getFilesWithTagsForEmbedding(this.getWordsFrom(1)).map(file => file.file)
   return `${files.map(file => file.filePath + "\n " + file.originalScrollCode.replace(/\n/g, "\n ") ).join("\n")}`
  }