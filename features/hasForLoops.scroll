codeWithHeader Example from <b>1 languages</b>: <a href="../concepts/jule.html">Jule</a>
 // Jule has for loops with while-next iterations
 let mut i = 0
 for i &lt; 10; i++ {
     // ...
 }

codeWithHeader Example from <b>1 languages</b>: <a href="../concepts/bog.html">bog</a>
 let mut sum = 0
 for let c in &quot;hellö wörld&quot;
   match c
       &quot;h&quot; =&gt; sum += 1
       &quot;e&quot; =&gt; sum += 2
       &quot;l&quot; =&gt; sum += 3
       &quot;ö&quot; =&gt; sum += 4
       &quot;w&quot; =&gt; sum += 5
       &quot;d&quot; =&gt; sum += 6

