codeWithHeader Example from <b>1 languages</b>: <a href="../concepts/swift.html">Swift</a>
 protocol MyProtocol {
 init(parameter: Int)
 var myVariable: Int { get set }
 var myReadOnlyProperty: Int { get }
 func myMethod()
 func myMethodWithBody()
 }
 extension MyProtocol {
 func myMethodWithBody() {
   // implementation goes here
   }
 }

codeWithHeader Example from <b>1 languages</b>: <a href="../concepts/kotlin.html">Kotlin</a>
 interface Named {
    val name: String
 }
 interface Person : Named {
     val firstName: String
     val lastName: String
     
     override val name: String get() = &quot;$firstName $lastName&quot;
 }

